
<%= render "layouts/errors", :object => @product %>
<%= form_for @product, url: new_product_path, html: { multipart: true } do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>
  <%= f.label :description %>
  <%= f.text_field :description %>
  <%= f.label :price %>
  <%= f.number_field :price %>
  <%= f.submit "Add" %>
<% end %>
